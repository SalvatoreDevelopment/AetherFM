{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to FFXIV",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}",
            "processName": "ffxiv_dx11",
            "justMyCode": false,
            "symbolSearchPath": "${workspaceFolder}/bin/Debug/net7.0"
        },
        {
            "name": "Build Plugin",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "dotnet",
            "args": ["build"],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole"
        }
    ]
} 